{
  "name": "hardhat-ts-plugin-boilerplate",
  "version": "0.0.1",
  "type": "module",
  "description": "Hardhat TypeScript plugin boilerplate",
  "repository": "github:nomiclabs/hardhat-ts-plugin-boilerplate",
  "author": "Nomic Labs LLC",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npm run lint:ts && npm run prettier:check && npm run typecheck",
    "lint:ts": "eslint",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write \"**/*.{js,json,md,ts,yml}\"",
    "prettier:check": "prettier --check --config \"./.prettierrc.yml\" --ignore-path \"./.prettierignore\" \"**/*.{js,json,md,ts,yml}\"",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "typecheck": "tsc --incremental false --noEmit"
  },
  "files": [
    "dist/src/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/chai": "^4.1.7",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.2",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "hardhat": "^2.0.0",
    "mocha": "^10.4.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "peerDependencies": {
    "hardhat": "^2.0.0"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0"
  }
}
